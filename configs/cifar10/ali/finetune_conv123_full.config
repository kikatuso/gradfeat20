dataset cifar10
data_path ./data/cifar10
nclass 10
batchsize 128
optim sgd
lr 1e-3
wd 5e-5
niter 50000
stepsize 10000
mode full
fnet_path ./models/pretrained/cifar10/ali/fnet1.pt
