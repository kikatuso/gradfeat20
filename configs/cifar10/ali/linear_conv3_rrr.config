dataset cifar10
data_path ./data/cifar10
nclass 10
batchsize 128
optim sgd
lr 1e-1
wd 5e-5
niter 50000
stepsize 10000
mode full
fnet_path ./models/pretrained/cifar10/ali/fnet0.pt
hnet_path ./models/pretrained/cifar10/ali/ntk_hnet0.pt
clf_path ./models/pretrained/cifar10/ali/ntk_clf0.pt
model_path ./models/benchmark/cifar10/ali/linear_conv3_rrr.net
freeze_hnet [1, 2, 3]
linearize_hnet 3
linearize_clf