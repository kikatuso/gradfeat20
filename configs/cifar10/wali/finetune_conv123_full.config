dataset cifar10
data_path ./data/cifar10
nclass 10
normalize
batchsize 128
optim adam
lr 1e-3
wd 0
niter 50000
stepsize 10000
mode full
fnet_path ./models/pretrained/cifar10/wali/fnet1.pt
